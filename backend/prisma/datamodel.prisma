type Route {
	id: ID! @id
	name: String! @unique
	createdAt: DateTime! @createdAt
	updatedAt: DateTime! @updatedAt
	stopList: [Stop]
}

type Stop {
	id: ID! @id
	name: String! @unique
	createdAt: DateTime! @createdAt
	updatedAt: DateTime! @updatedAt
	eventList: [Event]
}

type Event {
	id: ID! @id
	createdAt: DateTime! @createdAt
	updatedAt: DateTime! @updatedAt
	stop: Stop!
	route: Route!
	time: DateTime!
}
